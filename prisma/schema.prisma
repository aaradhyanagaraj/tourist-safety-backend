generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tourist {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
}

model ForeignTourist {
  id           Int      @id @default(autoincrement())
  name         String
  passportNo   String   @unique
  nationality  String
  visaType     String
  visaNumber   String
  travelStart  DateTime
  travelEnd    DateTime
  consent      Boolean
  createdAt    DateTime @default(now())
}
